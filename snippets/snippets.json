{
  "import": {
    "prefix": "impe",
    "body": "import Ember from 'ember';",
    "description": "Import Ember"
  },

  "importJS": {
    "prefix": "impj",
    "body": "import ${1:moduleName} from '${2:module}';$0"
  },

  "component": {
    "prefix": "component",
    "body": "import Ember from 'ember';\n\nconst { Component } = Ember;\n\nexport default Component.extend({\n});",
    "description": "Import Ember component"
  },

  "fullComponent": {
    "prefix": "fcomponent",
    "body": "import Ember from 'ember';\n\nconst { Component, computed, inject } = Ember;\n\nexport default Component.extend{(\n\n${1:ServiceName}: inject.service(),\n\n${2:computeName: computed('${3:property}', function({\n}}),\n\n});"
  },

  "componentDest": {
    "prefix": "componentdest",
    "body": "const { Component } = Ember;\n\nexport default Component.extend({\n});"
  },

  "alert": {
    "prefix": "alrt",
    "body": "alert(${1:obj});"
  },

  "inject": {
    "prefix": "inj",
    "body": "const { inject } = Ember;\n"
  },

  "serviceInject": {
    "prefix": "servi",
    "body": "${1:ServiceName}: inject.service($2),\n"
  },

  "destMostPopular": {
    "prefix": "destcom",
    "body": "const { Component, computed, inject } = Ember;\n"
  },

  "actions": {
    "prefix": "actions",
    "body": "actions: {\n\n}"
  },

  // Component Lifecycle

  "init": {
    "prefix": "init",
    "body": "init() {\n\tthis.super(...arguments);\n}"
  },


  "didInsertElement": {
    "prefix": "die",
    "body": "didInsertElement() {\n\tthis.super(...arguments);\n}"
  },

  "didRender": {
    "prefix": "dr",
    "body": "didRender() {\n\tthis.super(...arguments);\n}"
  },

  "didReceiveAttrs": {
    "prefix": "dra",
    "body": "didReceiveAttrs() {\n\tthis.super(...arguments);\n}"
  },

  // computed

  "computedDest": {
    "prefix": "compdest",
    "body": "const { computed } = Ember;\n"
  },

  "computed": {
    "prefix": "comp",
    "body": "${1:computeName}: computed('${2:dependentKey}', function() {\n\n}),"
  },

  "computedAlias": {
    "prefix": "alias",
    "body": "${1:computeName}: computed.alias('${2:dependentKey}'),"
  },

  "computedAnd": {
    "prefix": "and",
    "body": "${1:computeName}: computed.and('${2:dependentKey}', '${3:dependentKey}'),"
  },

  "computedBool": {
    "prefix": "bool",
    "body": "${1:computeName}: computed.bool('${2:dependentKey}'),"
  },

  "computedCollect": {
    "prefix": "collect",
    "body": "${1:computeName}: computed.collect('${2:dependentKey}', '${3:dependentKey}'),"
  },

  "computedEmpty": {
    "prefix": "empty",
    "body": "${1:computeName}: computed.empty('${2:dependentKey}'),"
  },

  "computedEqual": {
    "prefix": "equal",
    "body": "${1:computeName}: computed.equal('${2:dependentKey}', ${3:value}),"
  },

  "computedFilter": {
    "prefix": "filter",
    "body": "${1:computeName}: computed.filter('${2:dependentKey}', function(${3:item}, ${4:index}, ${5:array}) {\n\n}),"
  },

  "computedFilterBy": {
    "prefix": "filterBy",
    "body": "${1:computeName}: computed.filterBy('${2:dependentKey}', '${3:key}', ${4:value}),"
  },

  "computedGt": {
    "prefix": "gt",
    "body": "${1:computeName}: computed.gt('${2:dependentKey}', ${3:value}),"
  },

  "computedGte": {
    "prefix": "gte",
    "body": "${1:computeName}: computed.gte('${2:dependentKey}', ${3:value}),"
  },

   "computedIntersect": {
    "prefix": "intersect",
    "body": "${1:computeName}: computed.intersect('${2:array}', '${3:array}'),"
  },

  "computedLt": {
    "prefix": "Lt",
    "body": "${1:computeName}: computed.lt('${2:dependentKey}', ${3:value}),"
  },

  "computedLte": {
    "prefix": "lte",
    "body": "${1:computeName}: computed.lte('${2:dependentKey}', ${3:value}),"
  },

  "computedMap": {
    "prefix": "map",
    "body": "${1:computeName}: computed.map('${2:dependentKey}', function(${3:item}, ${4:index}) {\n\n}),"
  },

  "computedMapBy": {
    "prefix": "mapBy",
    "body": "${1:computeName}: computed.mapBy('${2:array}', '${3:key}'),"
  },

  "computedMatch": {
    "prefix": "match",
    "body": "${1:computeName}: computed.match('${2:dependentKey}', ${3:regexp}),"
  },

  "computedMax": {
    "prefix": "max",
    "body": "${1:computeName}: computed.max('${1:dependentKey}'),"
  },

   "computedMin": {
    "prefix": "min",
    "body": "${1:computeName}: computed.min('${1:dependentKey}'),"
  },

  "computedNone": {
    "prefix": "none",
    "body": "${1:computeName}: computed.none('${1:dependentKey}'),"
  },

  "computedNot": {
    "prefix": "not",
    "body": "${1:computeName}: computed.not('${1:dependentKey}'),"
  },

  "computedNotEmpty": {
    "prefix": "notEmpty",
    "body": "${1:computeName}: computed.notEmpty('${1:dependentKey}'),"
  },

  "computedOneWay": {
    "prefix": "oneWay",
    "body": "${1:computeName}: computed.oneWay('${1:dependentKey}'),"
  },

  "computedOr": {
    "prefix": "or",
    "body": "${1:computeName}: computed.or('${1:dependentKey}'),"
  },

  "computedSum": {
    "prefix": "sum",
    "body": "${1:computeName}: computed.sum('${1:dependentKey}'),"
  },

  "computedReadOnly": {
    "prefix": "readOnly",
    "body": "${1:computeName}: computed.readOnly('${1:dependentKey}'),"
  },

  "computedReads": {
    "prefix": "reads",
    "body": "${1:computeName}: computed.reads('${1:dependentKey}'),"
  },

  "computedSetDiff": {
    "prefix": "setDiff",
    "body": "${1:computeName}: computed.setDiff('${2:arrayA}', '${3:arrayB}'),"
  },

  "computedUnion": {
    "prefix": "union",
    "body": "${1:computeName}: computed.union('${2:arrayA}', '${3:arrayB}'),"
  },

  "computedUniq": {
    "prefix": "uniq",
    "body": "${1:computeName}: computed.uniq('${2:propertyKey}'),"
  },

  "computedUniqBy": {
    "prefix": "uniqBy",
    "body": "${1:computeName}: computed.uniqBy('${2:dependentKey}', '${3:propertyKey}'),"
  },

  //store

  "injectStore": {
    "prefix": "injstore",
    "body": "store: inject.service(),"
  },

  "getStore": {
    "prefix": "gstore",
    "body": "this.get('store')"
  },

  "findAll": {
    "prefix": "fall",
    "body": "this.get('store').findAll('${1:model}')"
  },

  "peekAll": {
    "prefix": "pall",
    "body": "this.get('store').peekAll('${1:model}')"
  },

  "query": {
    "prefix": "query",
    "body": "this.get('store').query('${1:model}', {})"
  },

  "findRecord": {
    "prefix": "find",
    "body": "this.get('store').find('${1:model}', $2id)"
  }

}
